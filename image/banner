   $(function(){
        if ($('.header').hasClass('insides')) {
           
                $('.header-body .header-list .header-nav-item-lk').css('color', '#000000');
                $('.header-body .header-logo img').attr('src', '/template/default/img/index/logo.svg')
                $('.head-language a').css('color', '#000000');
                $('.header-body .header-logo p').css('color', '#000000')
                $('.header-body .header-right .head-search img').attr('src', '/template/default/img/index/search_2.png')
                $('.head-language .fa').css('color', '#000000')
            }
    });
    function getScrollTop(){ return $('html').scrollTop() || $('body').scrollTop()}
	$(window).scroll(function(){  	

	   var scrollTop = getScrollTop();
            
            if (scrollTop > 10) {
                $('.header.indexs').addClass('is_header');
                // $('.header').css('background-image', 'none');

                $('.header.indexs .header-body .header-list .header-nav-item-lk').css('color', '#000000');
                // $('.head-contact a').css({
                // 	'color' : '#000000',
                // 	'border-color':'#000000',

                // });
                $('.header.indexs  .header-body .header-logo img').attr('src', '/template/default/img/index/logo.svg')
                $('.header.indexs .head-language a').css('color', '#000000');
                $('.header.indexs .header-body .header-logo p').css('color', '#000000')
                $('.header.indexs .header-body .header-right .head-search img').attr('src', '/template/default/img/index/search_2.png')
                $('.header.indexs .head-language .fa').css('color', '#000000')
            } else {
                $('.header.indexs ').removeClass('is_header');
                // $('.header').css('background-image', 'url(http://webtest.zjchuhai.com/jinchang/template/default/img/index/header_bg.png)');

                $('.header.indexs .header-body .header-list .header-nav-item-lk').css('color', '#fff');
                // $('.head-contact a').css({
                // 	'color' : '#fff',
                // 	'border-color':'#fff',

                // });
                $('.header.indexs .header-body .header-logo img').attr('src', '/template/default/img/index/logo1.svg')
                    
